<table>
  <tr>
    <th>Nome</th>
    <th>Token</th>
    <th>Email</th>
    <th>Data de admissão</th>
    <th>Valor disponível</th>
  </tr>
  <% rows.each do |row| %>
    <tr>
      <td><%= row['name'] + ' ' + row['surname'] %></td>
      <td><%= row['token'] %></td>
      <td><%= row['email'] %></td>
      <td><%= l Date.strptime(row['admission_date'], '%d-%m-%y') %></td>
      <td><%= number_to_currency row['available_amount'] %></td>
    </tr>
  <% end %>
</table>